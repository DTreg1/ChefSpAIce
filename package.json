{
  "name": "ChefSpAIce",
  "main": "client/index.js",
  "version": "1.0.0",
  "description": "ChefSpAIce - AI-Powered Kitchen Management",
  "author": "Darin Tregloan",
  "scripts": {
    "knip": "npx knip",
    "expo:clean": "npx expo prebuild --clean",
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "all:dev": "npm run expo:dev & npm run server:dev",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "build": "npm run server:build",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "eas:init": "npx eas-cli init",
    "eas:build": "npx eas-cli build",
    "build:web": "npx eas-cli build --platform web",
    "build:ios": "npx eas-cli build --platform ios",
    "build:android": "npx eas-cli build --platform android",
    "submit:ios": "npx eas-cli submit --platform ios",
    "submit:android": "npx eas-cli submit --platform android",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"{shared,server,client}/**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"{shared,server,client}/**/*.{js,ts,tsx,css,json}\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:auth": "jest auth-context",
    "test:subscription": "jest subscription",
    "test:storekit": "jest storekit",
    "test:storage": "jest storage.test",
    "test:sync": "jest sync-status",
    "test:notifications": "jest notifications",
    "test:instacart": "jest instacart",
    "test:voice": "jest voice",
    "test:nutrition": "jest nutrition",
    "test:shelf": "jest shelf-life",
    "test:recipe": "jest smart-recipe",
    "test:waste": "jest waste-reduction",
    "test:cookware": "jest cookware",
    "test:cooking": "jest cooking-terms",
    "test:tts": "jest useTextToSpeech"
