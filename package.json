    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@types/supertest": "^6.0.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
