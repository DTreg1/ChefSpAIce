{
  "expo": {
    "name": "ChefSpAIce",
    "slug": "chefspaice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./client/assets/images/icon.png",
    "scheme": "chefspaice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["client/assets/images/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chefspaice.chefspaice",
      "infoPlist": {
        "NSCameraUsageDescription": "ChefSpAIce needs camera access to scan barcodes and add food items to your inventory.",
        "NSPhotoLibraryUsageDescription": "ChefSpAIce would like access to your photo library to access select images for your food items.",
        "NSMicrophoneUsageDescription": "ChefSpAIce needs microphone access to use voice commands for hands-free recipe navigation and adding items to your inventory.",
        "NSFaceIDUsageDescription": "ChefSpAIce uses Face ID to quickly and securely unlock the app and protect your account.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#27AE60",
        "foregroundImage": "./client/assets/images/android-icon-foreground.png",
        "backgroundImage": "./client/assets/images/android-icon-background.png",
        "monochromeImage": "./client/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.chefspaice.chefspaice.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./client/assets/images/favicon.png",
      "bundler": "metro",
      "title": "ChefSpAIce - AI-Powered Kitchen Assistant"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./client/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#27AE60"
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "ChefSpAIce needs camera access to scan barcodes and add food items to your inventory."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./client/assets/images/icon.png",
          "color": "#27AE60"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "ChefSpAIce needs photo library access to select images for your food items."
        }
      ],
      "@sentry/react-native/expo"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "5b92d512-fd14-4e4f-b537-76dc95139451"
      }
    },
    "owner": "dtreg1"
  }
}
