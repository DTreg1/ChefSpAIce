# USDA FoodData Central API Configuration
USDA_FDC_API_KEY=your_usda_api_key_here

# Cache Configuration
CACHE_ENABLED=true
CACHE_MAX_SIZE=10000
CACHE_DEFAULT_TTL_DAYS=30

# Cache TTL Configuration (in milliseconds)
# 30 days for food data (stable data)
CACHE_TTL_FOOD=2592000000

# 7 days for search results (may change with new foods added)  
CACHE_TTL_SEARCH=604800000

# 90 days for nutrient data (very stable)
CACHE_TTL_NUTRIENTS=7776000000

# 14 days for branded foods (products may update)
CACHE_TTL_BRANDED=1209600000

# 14 days for barcode data
CACHE_TTL_BARCODE=1209600000

# Admin Configuration
ADMIN_EMAILS=admin@example.com,another-admin@example.com

# Database Configuration (provided by Replit)
DATABASE_URL=postgresql://...

# Other API Keys as needed
OPENAI_API_KEY=your_openai_key_here
STRIPE_SECRET_KEY=your_stripe_key_here