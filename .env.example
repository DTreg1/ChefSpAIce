# USDA FoodData Central API Configuration
USDA_FDC_API_KEY=your_usda_api_key_here

# Cache Configuration
CACHE_ENABLED=true                        # Enable/disable caching globally
CACHE_MAX_SIZE=10000                      # Maximum number of entries in cache
CACHE_DEFAULT_TTL_DAYS=30                 # Default TTL in days for cache entries

# Cache TTL Configuration (in days)
# Note: Values < 365 are treated as days, >= 365 as milliseconds for backward compatibility
CACHE_TTL_FOOD_DAYS=30                    # Food data (stable, rarely changes)
CACHE_TTL_SEARCH_DAYS=7                   # Search results (may change with new foods added)  
CACHE_TTL_NUTRIENTS_DAYS=90               # Nutrient data (very stable, scientific data)
CACHE_TTL_BRANDED_DAYS=14                 # Branded foods (products may update more frequently)
CACHE_TTL_BARCODE_DAYS=14                 # Barcode data (linked to branded products)

# Optional: Debug mode for verbose cache logging
CACHE_DEBUG=false                         # Enable detailed cache operation logging

# Admin Configuration
ADMIN_EMAILS=admin@example.com,another-admin@example.com

# Database Configuration (provided by Replit)
DATABASE_URL=postgresql://...

# Other API Keys as needed
OPENAI_API_KEY=your_openai_key_here
STRIPE_SECRET_KEY=your_stripe_key_here